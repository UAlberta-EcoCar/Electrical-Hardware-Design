    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cortex Debug - stlink",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/FCC-Src_V1_CMake.elf",
            "request": "launch",
            "type": "cortex-debug",
            "interface": "swd",
            "runToEntryPoint": "main",
            "servertype": "stlink",
            "preLaunchTask": "build",
            "showDevDebugOutput": "raw"
        },
        {
            "name": "Cortex Debug - stlink (attach)",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/FCC-Src_V1_CMake.elf",
            "request": "attach",
            "type": "cortex-debug",
            "interface": "swd",
            "runToEntryPoint": "main",
            "servertype": "stlink",
        }
    ]
}