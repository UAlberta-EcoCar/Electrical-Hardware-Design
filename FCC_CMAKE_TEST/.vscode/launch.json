{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cortex Debug - stlink",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/Debug/FCC_CMAKE_TEST.elf",
            "request": "launch",
            "type": "cortex-debug",
            "interface": "swd",
            "runToEntryPoint": "main",
            "servertype": "stlink",
            "preLaunchTask": "build",
            "showDevDebugOutput": "raw"
        },
        {
            "name": "Cortex Debug - stlink (attach)",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/Debug/FCC_CMAKE_TEST.elf",
            "request": "attach",
            "type": "cortex-debug",
            "interface": "swd",
            "runToEntryPoint": "main",
            "servertype": "stlink",
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 4
            }
        }
    ]
}